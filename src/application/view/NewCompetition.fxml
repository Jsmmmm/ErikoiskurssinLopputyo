<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox prefHeight="123.0" prefWidth="526.0" xmlns="http://javafx.com/javafx/8.0.60" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.controllers.NewCompetitionController">
   <children>
      <HBox prefHeight="70.0" prefWidth="526.0">
         <children>
            <Label prefHeight="38.0" prefWidth="161.0" text="Tournament name:">
               <font>
                  <Font size="14.0" />
               </font>
            </Label>
            <TextField fx:id="name" prefHeight="34.0" prefWidth="228.0" />
         </children>
         <VBox.margin>
            <Insets left="10.0" top="10.0" />
         </VBox.margin>
      </HBox>
      <HBox prefHeight="47.0" prefWidth="496.0" spacing="30.0">
         <children>
            <Button fx:id="save" mnemonicParsing="false" onAction="#createNewTournament" text="Save" />
            <Button fx:id="cancel" mnemonicParsing="false" onAction="#closeWindow" text="Cancel" />
         </children>
         <VBox.margin>
            <Insets left="10.0" />
         </VBox.margin>
      </HBox>
   </children>
</VBox>
