<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Menu?>
<?import javafx.scene.control.MenuBar?>
<?import javafx.scene.control.MenuItem?>
<?import javafx.scene.control.Separator?>
<?import javafx.scene.control.ToolBar?>
<?import javafx.scene.layout.VBox?>

<VBox prefHeight="75.0" prefWidth="1280.0" xmlns="http://javafx.com/javafx/8.0.60" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.controllers.MenuAndToolbarController">
   <children>
      <MenuBar>
        <menus>
          <Menu mnemonicParsing="false" text="File">
            <items>
              <MenuItem mnemonicParsing="false" text="Close" />
            </items>
          </Menu>
          <Menu mnemonicParsing="false" text="Edit">
            <items>
              <MenuItem mnemonicParsing="false" text="Delete" />
            </items>
          </Menu>
          <Menu mnemonicParsing="false" text="Help">
            <items>
              <MenuItem mnemonicParsing="false" text="About" />
            </items>
          </Menu>
        </menus>
      </MenuBar>
      <ToolBar prefHeight="50.0" prefWidth="200.0">
        <items>
          <Button fx:id="newCompetition" contentDisplay="CENTER" graphicTextGap="1.0" mnemonicParsing="false" prefHeight="40.0" prefWidth="40.0" />
            <Button fx:id="openSavedCompetition" contentDisplay="CENTER" graphicTextGap="1.0" mnemonicParsing="false" prefHeight="40.0" prefWidth="40.0" />
            <Button fx:id="saveCompetition" contentDisplay="CENTER" graphicTextGap="1.0" mnemonicParsing="false" prefHeight="40.0" prefWidth="40.0" />
            <Separator orientation="VERTICAL" prefHeight="40.0" prefWidth="20.0" />
            <Button fx:id="newPerson" contentDisplay="CENTER" graphicTextGap="1.0" mnemonicParsing="false" onAction="#newPersonPressed" prefHeight="40.0" prefWidth="40.0" />
            <Button fx:id="newTeam" contentDisplay="CENTER" graphicTextGap="1.0" mnemonicParsing="false" onAction="#newTeamPressed" prefHeight="40.0" prefWidth="40.0" />
            <Button fx:id="newSport" contentDisplay="CENTER" graphicTextGap="1.0" mnemonicParsing="false" onAction="#newSportPressed" prefHeight="40.0" prefWidth="40.0" />
            <Separator orientation="VERTICAL" prefHeight="40.0" prefWidth="20.0" />
            <Button contentDisplay="CENTER" mnemonicParsing="false" prefHeight="38.0" prefWidth="38.0" text="Button" />
            <Button contentDisplay="CENTER" mnemonicParsing="false" onAction="#printtaa" prefHeight="38.0" prefWidth="38.0" text="Button" />
            <Button fx:id="testButton" contentDisplay="CENTER" mnemonicParsing="false" onAction="#printtaa" prefHeight="38.0" prefWidth="38.0" text="Button" />
        </items>
         <VBox.margin>
            <Insets />
         </VBox.margin>
      </ToolBar>
   </children>
</VBox>
