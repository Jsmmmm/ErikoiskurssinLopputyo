<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.CheckBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<AnchorPane prefHeight="575.0" prefWidth="1030.0" xmlns="http://javafx.com/javafx/8.0.60" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.controllers.SportTabController">
   <children>
      <VBox layoutX="10.0" layoutY="10.0">
         <children>
            <HBox prefHeight="37.0" prefWidth="446.0">
               <children>
                  <Label prefHeight="31.0" prefWidth="42.0" text="Sport:" />
                  <Label fx:id="name" prefHeight="32.0" prefWidth="197.0" />
               </children>
            </HBox>
            <HBox prefHeight="31.0" prefWidth="446.0">
               <children>
                  <Label prefHeight="29.0" prefWidth="173.0" text="Series Included:" />
               </children>
            </HBox>
            <HBox prefHeight="31.0" prefWidth="446.0">
               <children>
                  <CheckBox fx:id="generalSeries" mnemonicParsing="false" prefHeight="29.0" prefWidth="108.0" text="General" />
               </children>
            </HBox>
            <HBox prefHeight="30.0" prefWidth="446.0">
               <children>
                  <CheckBox fx:id="mensSeries" mnemonicParsing="false" prefHeight="17.0" prefWidth="151.0" text="Men's Serie" />
                  <CheckBox fx:id="womensSeries" mnemonicParsing="false" text="Women's Serie" />
               </children>
            </HBox>
            <HBox prefHeight="31.0" prefWidth="446.0">
               <children>
                  <CheckBox fx:id="menU18Series" mnemonicParsing="false" prefHeight="17.0" prefWidth="151.0" text="Men's U18" />
                  <CheckBox fx:id="womenU18Series" mnemonicParsing="false" prefHeight="17.0" prefWidth="129.0" text="Women's U18" />
               </children>
            </HBox>
         </children>
      </VBox>
      <Button fx:id="addCompetitors" layoutX="10.0" layoutY="290.0" mnemonicParsing="false" onAction="#addCompetitorsPressed" prefHeight="25.0" prefWidth="152.0" text="Add Competitors" />
      <Button fx:id="removeCompetitors" layoutX="171.0" layoutY="290.0" mnemonicParsing="false" prefHeight="25.0" prefWidth="157.0" text="Remove Competitors" />
      <Button fx:id="deleteSport" layoutX="348.0" layoutY="290.0" mnemonicParsing="false" prefHeight="25.0" prefWidth="102.0" text="Delete Sport" />
   </children>
</AnchorPane>
