<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.CheckBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<AnchorPane prefHeight="405.0" prefWidth="671.0" xmlns="http://javafx.com/javafx/8.0.60" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.controllers.NewSportController">
   <children>
      <VBox layoutX="20.0" layoutY="20.0" prefHeight="246.0" prefWidth="575.0">
         <children>
            <HBox prefHeight="37.0" prefWidth="446.0">
               <children>
                  <Label prefHeight="31.0" prefWidth="81.0" text="Name" />
                  <TextField fx:id="sportName" prefHeight="25.0" prefWidth="157.0" promptText="Name Of the Sport" />
               </children>
            </HBox>
            <HBox prefHeight="31.0" prefWidth="446.0">
               <children>
                  <Label prefHeight="32.0" prefWidth="173.0" text="Series Included:" />
               </children>
            </HBox>
            <HBox prefHeight="31.0" prefWidth="446.0">
               <children>
                  <CheckBox fx:id="general" mnemonicParsing="false" prefHeight="29.0" prefWidth="108.0" text="General" />
               </children>
            </HBox>
            <HBox prefHeight="30.0" prefWidth="446.0">
               <children>
                  <CheckBox fx:id="men" mnemonicParsing="false" prefHeight="17.0" prefWidth="151.0" text="Men" />
                  <CheckBox fx:id="women" mnemonicParsing="false" text="Women's" />
               </children>
            </HBox>
            <HBox prefHeight="31.0" prefWidth="446.0">
               <children>
                  <CheckBox fx:id="menU18" mnemonicParsing="false" prefHeight="17.0" prefWidth="151.0" text="Men U18 " />
                  <CheckBox fx:id="womenU18" mnemonicParsing="false" prefHeight="17.0" prefWidth="129.0" text="Women's U18" />
               </children>
            </HBox>
         </children>
      </VBox>
      <Button fx:id="save" layoutX="40.0" layoutY="300.0" mnemonicParsing="false" onAction="#saveButton" prefHeight="44.0" prefWidth="122.0" text="Save" />
      <Button fx:id="cancel" layoutX="198.0" layoutY="300.0" mnemonicParsing="false" onAction="#closeWindow" prefHeight="44.0" prefWidth="122.0" text="Cancel" />
   </children>
</AnchorPane>
